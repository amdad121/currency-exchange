{
    "name": "amdad121/currency-exchange",
    "description": "This is a Laravel package to get currency rates.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1@dev"
    },
    "autoload": {
        "psr-4": {
            "Amdad121\\CurrencyExchange\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Amdad121\\CurrencyExchange\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amdad121\\CurrencyExchange\\CurrencyExchangeServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Amdadul Haq",
            "email": "amdadulhaq781@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev"
